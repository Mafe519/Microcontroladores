....................      case '\f' 	:	lcd_envia_byte(0,1); 
.................... 	  			delay_ms(2); 
.................... 				break; 
....................      case '\n'	: 
.................... 	 case '\r' 	:	lcd_pos_xy(1,2); 
.................... 	  			break; 
....................      case '\b' 	:	lcd_envia_byte(0,0x10); 
.................... 	  			break; 
....................      default	:	lcd_envia_byte(1,c); 
.................... 	  			break; 
....................    } 
.................... } 
